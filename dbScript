CREATE TABLE `Association` (
 `ID` int(11) NOT NULL AUTO_INCREMENT,
 `UserID` int(11) NOT NULL,
 `OrderID` int(11) NOT NULL,
 PRIMARY KEY (`ID`),
 KEY `Association_fk0` (`UserID`),
 KEY `Association_fk1` (`OrderID`)
);

CREATE TABLE `Cars` (
 `ID` int(11) NOT NULL AUTO_INCREMENT,
 `RegistrationNumber` varchar(300) COLLATE latin1_general_ci NOT NULL,
 `Brand` varchar(300) COLLATE latin1_general_ci NOT NULL,
 `Model` varchar(300) COLLATE latin1_general_ci NOT NULL,
 `GarageID` int(11) NOT NULL,
 PRIMARY KEY (`ID`),
 KEY `Cars_fk0` (`GarageID`)
);

CREATE TABLE `Garage` (
 `ID` int(11) NOT NULL AUTO_INCREMENT,
 `Location` varchar(300) COLLATE latin1_general_ci NOT NULL,
 PRIMARY KEY (`ID`)
);

CREATE TABLE `Orders` (
 `ID` int(11) NOT NULL AUTO_INCREMENT,
 `UserID` int(11) NOT NULL,
 `Description` varchar(3000) COLLATE latin1_general_ci NOT NULL,
 `State` varchar(300) COLLATE latin1_general_ci NOT NULL,
 `CreateDate` date NOT NULL,
 `ModificationDate` date DEFAULT NULL,
 `FinishDate` date DEFAULT NULL,
 `Location` varchar(300) COLLATE latin1_general_ci NOT NULL,
 `Destination` varchar(300) COLLATE latin1_general_ci NOT NULL,
 PRIMARY KEY (`ID`),
 KEY `Orders_fk0` (`UserID`)
);

CREATE TABLE `UserData` (
 `ID` int(11) NOT NULL AUTO_INCREMENT,
 `UserID` int(11) NOT NULL,
 `Name` varchar(300) COLLATE latin1_general_ci NOT NULL,
 `Surname` varchar(300) COLLATE latin1_general_ci NOT NULL,
 `Address` varchar(300) COLLATE latin1_general_ci NOT NULL,
 `Telephone` varchar(300) COLLATE latin1_general_ci NOT NULL,
 `City` varchar(300) COLLATE latin1_general_ci NOT NULL,
 PRIMARY KEY (`ID`),
 KEY `UserData_fk0` (`UserID`)
) ;

CREATE TABLE `Users` (
 `ID` int(11) NOT NULL AUTO_INCREMENT,
 `Login` varchar(30) COLLATE latin1_general_ci NOT NULL,
 `Password` varchar(30) COLLATE latin1_general_ci NOT NULL,
 `Email` varchar(30) COLLATE latin1_general_ci NOT NULL,
 `Rank` varchar(30) COLLATE latin1_general_ci NOT NULL,
 `CarID` int(11) DEFAULT NULL,
 PRIMARY KEY (`ID`),
 KEY `Users_fk0` (`CarID`)
);
